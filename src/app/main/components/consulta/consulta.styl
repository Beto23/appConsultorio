.container-consultas
  ul
    margin-bottom 1em
.fa-money
  color green
  font-size 1.3em !important
  cursor pointer

.container-get-consultas
  max-height: 25em;
  overflow: auto;
  &-table
    i
      cursor pointer
    .fa-ban
      color red
      margin-right 1em
    .fa-pencil
      color blue

.container-postConsulta
  position fixed
  top 0
  bottom 0
  left 0
  right 0
  background rgba(black,0.6)
  form
    position: relative;
    background-color: #fff;
    background-clip: padding-box;
    border: 1px solid #999;
    border: 1px solid rgba(0,0,0,.2);
    border-radius: 6px;
    outline: 0;
    box-shadow: 0 3px 9px rgba(0,0,0,.5);
    padding 2em
